@model SettingViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";

}

<div class="container-fluid">

    <a asp-action="create" class="btn btn-primary">Create</a>
    <table class="table">
        <thead>
            <tr class="row">
                <th scope="col" class="col-md-1">#</th>
                <th scope="col" class="col-md-3">About</th>
                <th scope="col" class="col-md-3">Adress</th>
                <th scope="col" class="col-md-3">Number</th>
                <th scope="col" class="col-md-3">TestimonialAbout</th>
                <th scope="col" class="col-md-3">TestimonialTitle</th>
                <th scope="col" class="col-md-3">VideoUrl</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Settings)
            {
            <tr class="row">
                <th scope="row" class="col-md-1"> </th>
                <td class="col-md-3">@item.About</td>
                <td class="col-md-3">@item.Adress</td>
                <td class="col-md-3">@item.Number</td>
                <td class="col-md-3">@item.TestimonialAbout</td>
                <td class="col-md-3">@item.TestimonialName</td>
                <td class="col-md-3">@item.TestimonialTitle</td>
                <td class="col-md-3">@item.VideoUrl</td>
     
                <td class="col-md-3">
                    <a asp-action="edit" asp-route-id="@item.Id" class="btn btn-info">Edit</a>
                    <a asp-action="delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
                </td>
            </tr>

            }

        </tbody>
    </table>

</div>
